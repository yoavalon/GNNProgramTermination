import time

x = 3
y = 19
if y > 11 : 
  for i in range(6) : 
    for i in range(1) : 
      for i in range(12) : 
        if y > 1 : 
          time.sleep(2)
          while y > x : 
            for i in range(16) : 
              for i in range(1) : 
                time.sleep(2)
                x -=1