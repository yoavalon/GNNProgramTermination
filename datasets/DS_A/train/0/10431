import time

x = 10
y = 13
for i in range(14) : 
  for i in range(1) : 
    time.sleep(2)
    for i in range(14) : 
      while y > x : 
        for i in range(6) : 
          while y > x : 
            while y > x : 
              for i in range(18) : 
                time.sleep(2)
                x -=1